    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Flask",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "env": {
                "FLASK_APP": "flask-demo/app.py",
                "FLASK_DEBUG": "1"
            },
            "args": [
                "run",
                "--no-debugger",
                "--no-reload"
            ],
            "jinja": true,
            "justMyCode": true
        },
        {
            "name": "Python: Module",
            "type": "python",
            "request": "launch",
            "module": "enter-your-module-name",
            "justMyCode": true
        },
        {
            "name": "Python: Test search",
            "type": "python",
            "request": "launch",
            "module": "pytest",
            "args": [
                "tests/test_search.py",
                "--pdb"
            ],
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "name": "バージョン表示",
